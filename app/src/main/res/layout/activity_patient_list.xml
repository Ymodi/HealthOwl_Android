<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_patient_list"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="m2y.centennial.healthowl.patientList">

    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorButtonNormal"
        android:theme="?attr/actionBarTheme"
        android:minHeight="?attr/actionBarSize"
        android:id="@+id/my_Detail_toolbar"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"/>
    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_below="@+id/my_Detail_toolbar"
        android:layout_alignParentStart="true" android:layout_marginStart="123dp"
        android:layout_marginTop="111dp" android:id="@+id/toPatientLink"/>


</RelativeLayout>
